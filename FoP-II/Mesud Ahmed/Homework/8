
#include<iostream>

using namespace std;
bool isPrime(int a);
int main()
{   int num;

    cout<<"enter a number between 1 and 1000"<<endl;
    cin>>num;
    if (num >1 && num<1000)
    {
    for(int i=2;i<num/2;i++)
    {
        if (num%i==0)
       { cout<<"not a prime number"<<endl;
       break;
       }
      
        else
         cout<<" true"<<endl;

    }

    }
    else 
    cout<<"invalid input"<<endl;
}
